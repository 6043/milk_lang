int! num = input

real... guess[20]

while num != 0 do {

    guess[0] = 1.0

    for int! i = 0; i < 19; i = i + 1 do {
        real! add_part = num / guess[i]
        guess[i+1] = guess[i] + add_part
        guess[i+1] = guess[i+1] / 2
    }

    for int! j = 0; j < 20; j = j + 1 do
    {
        moo guess[j]
    }

    num = input
}